{
    "name": "react-widgets",
    "version": "0.1.0",
    "private": true,

    "scripts": {
        "build:widgets": "ts-node -T --swc  -e 'require(\"./src/WidgetBuilder\").default.build().catch(console.error)'",
        "refresh:widgets": "ts-node -T --swc  -e 'require(\"./src/WidgetBuilder\").default.refresh().catch(console.error)'",
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "@parcel/config-default": "^2.7.0",
        "@parcel/core": "^2.7.0",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^27.5.2",
        "@types/node": "^16.11.62",
        "@types/react": "^18.0.21",
        "@types/react-dom": "^18.0.6",
        "axios": "^0.27.2",
        "bootstrap": "^5.2.1",
        "mobx": "^6.6.2",
        "mobx-react-lite": "^3.4.0",
        "react": "^18.2.0",
        "react-bootstrap": "^2.5.0",
        "react-dom": "^18.2.0",
        "react-icons": "^4.4.0",
        "react-scripts": "5.0.1",
        "short-number": "^1.0.7",
        "typescript": "^4.8.4",
        "web-vitals": "^2.1.4",
        "wouter": "^2.8.0-alpha.2"
    },
    "devDependencies": {
        "@swc/core": "^1.3.4",
        "parcel-bundler": "^1.12.5",
        "template-file": "^6.0.1",
        "ts-node": "^10.9.1"
    },
    "proxy": "http://localhost:8080"
}
